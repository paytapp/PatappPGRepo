package com.paymentgateway.phonePe;

import com.fasterxml.jackson.annotation.JsonGetter;
import com.fasterxml.jackson.annotation.JsonIgnoreProperties;
import com.fasterxml.jackson.annotation.JsonInclude;

@JsonIgnoreProperties(ignoreUnknown = true)
@JsonInclude(JsonInclude.Include.NON_NULL)
public class StatusCheckPaymentInstrumentBean {

	public String type;
	public String utr;
	public String cardType;
	public String pgTransactionId;
	public String bankTransactionId;
	public String pgAuthorizationCode;
	public String arn;
	public String bankId;
	public String pgServiceTransactionId;
	
	@JsonGetter("type")
	public String getType() {
		return type;
	}
	
	public void setType(String type) {
		this.type = type;
	}
	
	@JsonGetter("utr")
	public String getUtr() {
		return utr;
	}
	
	public void setUtr(String utr) {
		this.utr = utr;
	}
	
	@JsonGetter("cardType")
	public String getCardType() {
		return cardType;
	}
	
	public void setCardType(String cardType) {
		this.cardType = cardType;
	}
	
	@JsonGetter("pgTransactionId")
	public String getPgTransactionId() {
		return pgTransactionId;
	}
	
	public void setPgTransactionId(String pgTransactionId) {
		this.pgTransactionId = pgTransactionId;
	}
	
	@JsonGetter("bankTransactionId")
	public String getBankTransactionId() {
		return bankTransactionId;
	}
	
	public void setBankTransactionId(String bankTransactionId) {
		this.bankTransactionId = bankTransactionId;
	}
	
	@JsonGetter("pgAuthorizationCode")
	public String getPgAuthorizationCode() {
		return pgAuthorizationCode;
	}
	
	public void setPgAuthorizationCode(String pgAuthorizationCode) {
		this.pgAuthorizationCode = pgAuthorizationCode;
	}
	
	@JsonGetter("arn")
	public String getArn() {
		return arn;
	}
	
	public void setArn(String arn) {
		this.arn = arn;
	}
	
	@JsonGetter("bankId")
	public String getBankId() {
		return bankId;
	}
	
	public void setBankId(String bankId) {
		this.bankId = bankId;
	}
	
	@JsonGetter("pgServiceTransactionId")
	public String getPgServiceTransactionId() {
		return pgServiceTransactionId;
	}
	
	public void setPgServiceTransactionId(String pgServiceTransactionId) {
		this.pgServiceTransactionId = pgServiceTransactionId;
	}
	
	
}
